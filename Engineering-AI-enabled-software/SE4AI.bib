  booktitle = {2021 {{IEEE}}/{{ACM}} 43rd International Conference on Software Engineering: {{Companion}} Proceedings ({{ICSE-Companion}})},
  author = {Vakkuri, Ville and Kemell, Kai-Kristian and Abrahamsson, Pekka},
