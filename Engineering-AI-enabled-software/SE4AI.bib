  author = {{Mart{\'i}nez-Fern{\'a}ndez}, Silverio and Bogner, Justus and Franch, Xavier and Oriol, Marc and Siebert, Julien and Trendowicz, Adam and Vollmer, Anna Maria and Wagner, Stefan},
  year = {2022},
