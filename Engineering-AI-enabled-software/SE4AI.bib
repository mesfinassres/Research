@article{singh2022:extensive,
  title = {Extensive Performance Analysis of {{OpenDayLight}} ({{ODL}}) and {{Open Network Operating System}} ({{ONOS}}) {{SDN}} Controllers},
  author = {Singh, Avtar and Kaur, Navjot and Kaur, Harpreet},
  year = {2022},
